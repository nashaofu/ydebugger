{
  "name": "ydebugger",
  "version": "0.0.2",
  "description": "为平板设备web开发者工具，可使用 Github Codespaces & Codeserver 进行web开发。",
  "main": "index.js",
  "bin": {
    "ydebugger": "./ydebugger.js"
  },
  "files": [
    "lib",
    "ydebugger.js",
    "devtools-frontend/devtools-frontend"
  ],
  "scripts": {
    "dev": "tsc --watch --sourceMap",
    "build": "npm run lint && npm run clean && tsc",
    "lint": "eslint . --ext .js,.ts --fix",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/ydebugger.git"
  },
  "keywords": [
    "devtools",
    "web",
    "debugger",
    "inspector",
    "remote",
    "Codeserver",
    "Codespaces"
  ],
  "author": "nashaofu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nashaofu/ydebugger/issues"
  },
  "homepage": "https://github.com/nashaofu/ydebugger#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "detect-port": "^1.3.0",
    "express": "^4.17.2",
    "http-proxy-middleware": "^2.0.2",
    "open": "^8.4.0",
    "puppeteer": "^13.1.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.2",
    "@types/express": "^4.17.13",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
