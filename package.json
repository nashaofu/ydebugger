{
  "name": "ydebugger",
  "version": "0.1.2",
  "description": "让你的平板不再吃灰，不再买前生产力，买后爱奇艺，让 Github Codespace & Codeserver 也能在平板设备上进行 Web 开发调试。",
  "main": "index.js",
  "bin": {
    "ydebugger": "./ydebugger.js"
  },
  "files": [
    "lib",
    "polyfill",
    "index.html",
    "ydebugger.js"
  ],
  "scripts": {
    "dev": "tsc --watch --sourceMap",
    "build": "npm run lint && npm run clean && tsc",
    "lint": "eslint . --ext .js,.ts --fix",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/ydebugger.git"
  },
  "keywords": [
    "web",
    "chrome",
    "devtools",
    "debugger",
    "inspector",
    "remote",
    "Codeserver",
    "Codespaces"
  ],
  "author": "nashaofu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nashaofu/ydebugger/issues"
  },
  "homepage": "https://github.com/nashaofu/ydebugger#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "detect-port": "^1.3.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.3",
    "open": "^8.4.0",
    "puppeteer": "^2.1.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.2",
    "@types/express": "^4.17.13",
    "@types/puppeteer": "^5.4.4",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
